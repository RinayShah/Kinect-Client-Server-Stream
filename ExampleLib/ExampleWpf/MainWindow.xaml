<Window x:Class="ExampleWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExampleWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="LoadedWindow">
    <Grid>
        <TextBox x:Name="TextBoxInput" HorizontalAlignment="Left" Height="23" Margin="25,20,0,0" TextWrapping="Wrap" Text="Input" VerticalAlignment="Top" Width="120" KeyDown="TextBoxInputKeyDown"/>
        <TextBox x:Name="TextBoxOutput" HorizontalAlignment="Left" Height="23" Margin="25,58,0,0" TextWrapping="Wrap" Text="Output" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <TextBox x:Name="TextBoxDebug" HorizontalAlignment="Left" Height="150" Margin="25,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>

    </Grid>
</Window>
